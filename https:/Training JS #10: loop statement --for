function pickIt(arr){
  var odd=[],even=[];

  
    for ( let i = 0;  i< arr.length;i++ ){
    if ( arr[i] % 2 === 0){
      even.push(arr[i]);
    }
    }
    for ( let j = 0;  j< arr.length;j++ ){ 
    if ( arr[j] %2 !== 0) {
    odd.push(arr[j]);

    }
    }
    ;
 return [odd,even];

}
