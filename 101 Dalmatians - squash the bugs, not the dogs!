//https://www.codewars.com/users/koshka2008/completed_solutions
function howManyDalmatians(number){
  
  var dogs = ["Hardly any", "More than a handful!","Woah that's a lot of dogs!", "101 DALMATIANS!!!"];
  
  return (number >= 1 && number <= 10) ? dogs[0]:(number >=11 && number <= 51) ? dogs[1] :(number >=52 && number <=100)? dogs[2]: (number = 101)?  dogs[3] : dogs[0];
  
}
