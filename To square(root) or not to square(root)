function squareOrSquareRoot(array) {
  let newArray = [];
  let exp ;
  for (let i = 0; i < array.length; i++){
      if(Math.sqrt(array[i]) % 1 === 0 )
    exp = Math.sqrt( array[i]);
    else if (Math.sqrt(array[i]) % 1 !== 0)
      exp = Math.pow(array[i], 2);
    newArray.push(exp);
 }   
  return newArray;
}
